{
  "name": "macropay",
  "version": "2.0.0",
  "main": "electron/dist/main.js",
  "author": "grupomacro",
  "description": "Phoenix Suite",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-qa": "ng build --configuration=quality-assurance",
    "build-prod": "ng build --configuration=production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron-rebuild": "ng build --base-href \"./\" && tsc -p electron && electron ./electron/dist/main.js",
    "electron-rebuild-qa": "ng build --configuration=quality-assurance --base-href \"./\" && tsc -p electron && electron ./electron/dist/main.js",
    "electron": "electron ./electron/dist/main.js",
    "electron-build": "ng build --prod && npm run electron",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "build-e": "electron-builder build --win --publish never",
    "deploy-e": "electron-builder build --win --publish always",
    "package": "electron-packager . --overwrite",
    "start-proxy": "ng serve --proxy-config proxy.conf.json",
    "init-proxy": "ng serve --proxy-config ./proxy.conf.json",
    "packager64": "electron-packager ./ macropay --platform=win32 --arch=x64 --out ./OutApp --app-version 2.0.0 --overwrite",
    "package-arch": "electron-packager . macropay --platform=win32 --arch=x64 --out=release/package",
    "package-arch-over": "electron-packager . macropay --platform=win32 --arch=x64 --out=release/package --app-version 2.0.0 --overwrite",
    "build-winstaller": "node ./build.js"
  },
  "postinstall": "electron-builder install-app-deps",
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.6",
    "@angular/common": "~11.2.6",
    "@angular/compiler": "~11.2.6",
    "@angular/core": "~11.2.6",
    "@angular/forms": "~11.2.6",
    "@angular/platform-browser": "~11.2.6",
    "@angular/platform-browser-dynamic": "~11.2.6",
    "@angular/router": "~11.2.6",
    "@fullcalendar/core": "5.5.0",
    "@fullcalendar/daygrid": "5.5.0",
    "@fullcalendar/interaction": "5.5.0",
    "@ng-bootstrap/ng-bootstrap": "^9.1.0",
    "@swimlane/ngx-datatable": "19.0.0",
    "@types/w3c-web-usb": "^1.0.4",
    "bootstrap": "^4.6.0",
    "chart.js": "^2.9.4",
    "custom-electron-titlebar": "^3.2.6",
    "d3-scale": "^3.2.4",
    "devextreme": "^20.2.6",
    "devextreme-angular": "^20.2.6",
    "dropzone": "^5.8.1",
    "electron-builder-squirrel-windows": "^22.11.11",
    "electron-log": "^4.4.1",
    "electron-pos-printer": "^1.2.4",
    "electron-squirrel-startup": "^1.0.0",
    "electron-titlebar": "0.0.3",
    "electron-updater": "^4.3.9",
    "express": "^4.17.1",
    "file-system": "^2.2.2",
    "fs": "^0.0.1-security",
    "grunt": "^1.4.1",
    "grunt-electron-installer": "^2.1.0",
    "html2canvas": "^1.1.4",
    "jquery": "^3.6.0",
    "jspdf": "^2.3.1",
    "list.js": "^2.3.1",
    "mobius1-selectr": "^2.4.13",
    "moment": "^2.29.1",
    "ng-particles": "^2.14.1",
    "ng-thermal-print": "^1.0.3",
    "ngx-bootstrap": "^6.2.0",
    "ngx-chips": "2.1.0",
    "ngx-clipboard": "13.0.1",
    "ngx-mask": "^12.0.0",
    "ngx-perfect-scrollbar": "10.0.1",
    "ngx-printer": "^0.9.2",
    "ngx-spinner": "^11.0.1",
    "ngx-toastr": "12.0.0",
    "nouislider": "^14.6.4",
    "npm-install-peers": "^1.2.2",
    "path": "^0.12.7",
    "popper.js": "^1.16.1",
    "print-js": "^1.6.0",
    "protobufjs": "^6.11.2",
    "quill": "^1.3.7",
    "rxjs": "~6.6.0",
    "rxjs-compat": "^6.6.7",
    "steam": "^1.4.1",
    "sweetalert2": "^10.15.7",
    "tslib": "^2.0.0",
    "tsparticles": "^1.31.1",
    "update-electron-app": "^2.0.1",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.5",
    "@angular/cli": "~11.2.5",
    "@angular/compiler-cli": "~11.2.6",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "electron": "^13.1.8",
    "electron-builder": "^22.10.5",
    "electron-packager": "^15.3.0",
    "electron-winstaller": "^2.7.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  },
  "build": {
    "appId": "com.grupomacro.vds",
    "productName": "Phoenix Suite",
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "nsis": {
      "warningsAsErrors": false,
      "installerIcon": "build/icon.ico",
      "runAfterFinish": false,
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "include": "build/installer.nsh"
    },
    "publish": [
      {
        "provider": "generic",
        "url": "http://localhost:8050/releases/"
      }
    ]
  },
  "files": [
    "*.js",
    "build",
    "node_modules",
    "dist/**/*",
    "package.json",
    "public",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/sapgm/sd-servicios-front.git"
  }
}
